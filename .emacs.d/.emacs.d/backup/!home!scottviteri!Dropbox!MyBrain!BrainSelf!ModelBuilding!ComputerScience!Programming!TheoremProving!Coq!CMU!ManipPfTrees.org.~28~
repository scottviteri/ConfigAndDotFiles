def getMotifMatches(motif_tree, tree):
    # can make this much more efficient
    all_subtrees = genAllSubtrees(tree, len(motif_tree))
    subtree_candidates = filter(lambda subt: len(subt) == len(motif_tree), all_subtrees)
    return list(filter(lambda subt: isMatchTrees(motif_tree, subt), subtree_candidates))

def getMotifMatches2(motif_tree, tree):
    r1, r2 = getRoot(motif_tree), getRoot(tree)
    return getMotifMatches2Helper(r1, motif_tree, r2, tree)

def getMotifMatches2Helper(motif_root, motif_tree, tree_root, tree):
    """
    Example: a0_b1, a2_b3_a4b5
    # base: [{b3},{b5}]
    # ind_step: [{b3},{b5}}] -> [{a2,b3},{a4,b5}]
    """
    motif_root_name = getNodeName(motif_root)
    motif_root_children = getChildrenNodes(getNodeId(motif_root), motif_tree)
    tree_root_children = getChildrenNodes(getNodeId(tree_root), tree)
    if len(motif_root_children) == 0: #base case -- motif root is leaf
        print(list(map(lambda x: {x}, filter(lambda node: getNodeName(node) == motif_root_name, tree))))
        return map(lambda x: {x}, filter(lambda node: getNodeName(node) == motif_root_name, tree))
        #return {tree_root}
    else: # inductive step
        """
        return [getMotifMatches2Helper(motif_root_child, motif_tree, tree_root_child, tree)
                for motif_root_child in motif_root_children for tree_root_child in tree_root_children
                    if getNodeName(motif_root) == getNodeName(tree_root)]
        """

        for motif_root_child in motif_root_children:
            for tree_root_child in tree_root_children:
                if getNodeName(motif_root_child) == getNodeName(tree_root_child):
                    #getMotifMatches2Helper(motif_root_child, motif_tree, tree_root_child, tree)
                    return [x.union({getParentNode(getNodeId(tree_root_child),tree)}) for x in getMotifMatches2Helper(motif_root_child, motif_tree, tree_root_child, tree)]


def countMotifMatches(motif_tree, tree):
    # can make this much more efficient
    return len(getMaxDisjointTrees(getMotifMatches(motif_tree, tree)))

def isMotifTree(motif_tree, tree):
    return countMotifMatches(motif_tree, tree) >= 2

# [a, b] should NOT be a motif of [a, b, b]
assert(not isMotifTree(a_b, a_b_b))

# [a, b] SHOULD be a motif of [a, [a,b], [a,b]]
assert(isMotifTree(a_b, a_ab_ab))

# [a, b] SHOULD be a motif of [a, b, [a, b]]
assert(isMotifTree(a_b, a_b_ab))


def generateMotifs(tree, max_subtree_size):
    subtrees = list(genAllSubtrees(tree, max_subtree_size)) #make sure this is not generated many times
    potential_motifs = [] # collection of subtrees of a length with different structure
    for s in filter(lambda t: len(t)>1, subtrees): #non-trivial subtree
        # make potential motifs unique up to pattern structure
        if not any([isMatchTrees(s,t) for t in filter(lambda m: len(m)==len(s), potential_motifs)]):
            potential_motifs.append(s)
    return list(filter(lambda subt: isMotifTree(subt, tree), potential_motifs))

    """
_and_num_matches = list(map(lambda pattern: (pattern, getNumMatches(pattern, subtrees)),
                                    potential_motifs))
    motifs_with_multiple_matches_and_num_matches =  list(filter(lambda p: p[1] >= 2, motifs_and_num_matches))
    return list(motifs_with_multiple_matches_and_num_matches)
    """

assert(generateMotifs(a_b_b, 2) == [])

motif_ab = generateMotifs(a_ab_ab, 2)[0]
assert(isMatchTrees(motif_ab, a_b))

m_ab = generateMotifs(a_b_ab, 2)[0]
assert(isMatchTrees(m_ab, a_b))
assert(isMatchTrees(generateMotifs(a_b_ab, 2)[0],
                    generateMotifs(a_ab_ab, 2)[0]))

