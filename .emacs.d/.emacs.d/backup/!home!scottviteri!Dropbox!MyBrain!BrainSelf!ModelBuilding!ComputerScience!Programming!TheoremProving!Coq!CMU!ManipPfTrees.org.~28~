#+BEGIN_SRC python :session

def step(node, tree):
    node_id = getNodeId(node)
    if node[0]:
        return getChildrenNodes(node_id, tree).union({getParentNode(node,tree)})
    else: return getChildrenNodes(node_id, tree).union(getChildrenNodes(None, tree))

#eg (3, 'O', 5) (7, 'Top.ev_4', 8) in subNats(even_4_tree)

def dist(n1,n2,tree):
    visited_set = set()
    frontier_queue = [n1]
    d = 0
    while frontier_queue:
        current_node = frontier_queue.pop(0)
        visited_set.add(current_node)
        if current_node == n2:
            return d
        print('c', current_node, d)
        for next_step in step(current_node, tree):
            assert(type(next_step) is tuple)
            if next_step not in visited_set:
                frontier_queue.append(next_step)
        d += 1
    return 'Destination not found'

#assert(dist((3,'O',5), (7,'Top.ev_4',8), subNats(even_4_tree)) == 5)
