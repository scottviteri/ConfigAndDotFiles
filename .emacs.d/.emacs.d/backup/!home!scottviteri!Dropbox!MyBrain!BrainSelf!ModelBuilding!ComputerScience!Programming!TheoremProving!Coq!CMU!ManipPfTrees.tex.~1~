\end{verbatim}

\begin{verbatim}

def listsToTree(lst):
    if lst:
        t = Tree(lst[0])
        for x in lst[1:]:
            if isinstance(x,str):
                t.addChild(x)
            elif isinstance(x,list):
                if x:
                    t.addChild(listsToTree(x))
    return t

p1, p2 = parse_parentheses(s1), parse_parentheses(s2)
t1, t2 = listsToTree(p1), listsToTree(p2)
print_tree(t1)

\end{verbatim}

